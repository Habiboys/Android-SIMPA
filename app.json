{
  "expo": {
    "name": "SIMPA",
    "newArchEnabled": true,
    "slug": "simpa",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logosuralaya.png",
    "splash": {
      "image": "./assets/logosuralaya.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "plugins": [
      [
       "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos.",
          "cameraPermission": "The app needs access to your camera."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cvsuralaya.simpa"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logosuralaya.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.cvsuralaya.simpa",
      "permissions": [
    "android.permission.CAMERA",
    "android.permission.READ_EXTERNAL_STORAGE",
    "android.permission.WRITE_EXTERNAL_STORAGE",
    "android.permission.MEDIA_LIBRARY"
  ]
    },
    "extra": {
      "eas": {
        "projectId": "5eb9007b-b8fc-4dfb-8800-a7d03cf7add2"
      }
    }
  }
}
